void setup()
{
  pinMode(3, OUTPUT); //asettaa pinnin OUTPUT-pinniksi
  pinMode(4, OUTPUT); //asettaa pinnin OUTPUT-pinniksi
  pinMode(5, OUTPUT); //asettaa pinnin OUTPUT-pinniksi
}

void loop()
{
  digitalWrite(3, HIGH);
  delay(300); // Valo palaa annetun ajan sekunteina
  digitalWrite(3, LOW); // Valo sammuu
  delay(300);
  
  digitalWrite(3, HIGH);
  delay(300); // Valo palaa annetun ajan sekunteina
  digitalWrite(3, LOW); // Valo sammuu
  delay(300);
  
  digitalWrite(3, HIGH);
  delay(300); // Valo palaa annetun ajan sekunteina
  digitalWrite(3, LOW); // Valo sammuu
  delay(300);
  
  digitalWrite(3, HIGH);
  digitalWrite(4, HIGH);
  digitalWrite(5, HIGH);
  delay(300); // Valo palaa annetun ajan sekunteina
  digitalWrite(3, LOW); // Valo sammuu
  digitalWrite(4, LOW); // Valo sammuu
  digitalWrite(5, LOW); // Valo sammuu
  delay(300);
  
}
